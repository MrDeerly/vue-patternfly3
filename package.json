{
  "name": "vue-patternfly",
  "version": "0.0.17",
  "description": "PatternFly components for Vue.js 2",
  "main": "dist/vue-patternfly.min.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "purge": "rm -rf dist/*",
    "compile": "webpack --mode production --progress && mv dist/vue-patternfly.js dist/vue-patternfly.min.js && webpack --mode development --progress",
    "dev": "webpack --mode development -w --progress",
    "build": "npm run purge && npm run compile",
    "lint": "eslint src/*.js",
    "prepublish": "npm run lint && npm run build"
  },
  "files": [
    "dist/",
    "src/"
  ],
  "keywords": [
    "vue",
    "patternfly"
  ],
  "author": "Massimiliano Torromeo <massimiliano.torromeo@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mtorromeo/vue-patternfly.git"
  },
  "bugs": {
    "url": "https://github.com/mtorromeo/vue-patternfly/issues"
  },
  "homepage": "https://github.com/mtorromeo/vue-patternfly#readme",
  "engines": {
    "node": ">= 6.0"
  },
  "dependencies": {
    "c3": "^0.4.11",
    "es6-promise": "^4.2.5",
    "patternfly": "^3.14.0",
    "tether-tooltip": "^1.2.0",
    "uiv": "^0.27.0",
    "vue": "^2.5.13"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-async-to-generator": "^7.1.0",
    "@babel/plugin-transform-destructuring": "^7.1.3",
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/runtime": "^7.1.2",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "css-loader": "^1.0.0",
    "eslint": "^5.7.0",
    "eslint-friendly-formatter": "^4.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-vue": "^4.4.0",
    "exports-loader": "^0.7.0",
    "html-loader": "^0.5.1",
    "imports-loader": "^0.8.0",
    "lodash": "^4.17.4",
    "mini-css-extract-plugin": "^0.4.0",
    "portal-vue": "^1.3.0",
    "resize-observer-polyfill": "^1.5.0",
    "style-loader": "^0.23.1",
    "vue-functional-data-merge": "^2.0.6",
    "vue-loader": "^15.4.2",
    "vue-portal": "^1.0.0",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.1.2"
  }
}
